<div class="row">
    <div class="col-md-4">
        <div class="panel-group" id="publicAccordion">
            <!-- accordion 1 -->
            @{int i = 0;}
            @foreach(var note in ViewBag.publicList) {
                i++;
                    <div class="panel panel-success">
                        <div class="panel-heading">
                            <!-- panel-heading -->
                            <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#publicAccordion" href=@("#publicAccordion" + i)>
                                    @note.Title
                                </a>
                                <a href="/note/edit/@note.NoteID" type="button" class="btn btn-sm">修改</a>
                                <a href="/note/delete/@note.NoteID" type="button" class="btn btn-sm">删除</a>
                            </h4>
                        </div>
                        <!-- panel body -->
                        <div id=@("publicAccordion" + i) class="panel-collapse collapse">
                            <div class="panel-body">
                                @note.Description
                            </div>
                        </div>
                    </div>
            }
        </div>
    </div>
    <div class="col-md-4">
        <div class="panel-group" id="friendlyAccordion">
            @{i = 0;}
            @foreach (var note in ViewBag.friendlyList)
            {
                i++;
                <div class="panel panel-warning">
                    <div class="panel-heading">
                        <!-- panel-heading -->
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#friendlyAccordion" href=@("#friendlyAccordion" + i)>
                                @note.Title
                            </a>
                            <a href="/note/edit/@note.NoteID" type="button" class="btn btn-sm">修改</a>
                            <a href="/note/delete/@note.NoteID" type="button" class="btn btn-sm">删除</a>
                        </h4>
                    </div>
                    <!-- panel body -->
                    <div id=@("friendlyAccordion" + i) class="panel-collapse collapse">
                        <div class="panel-body">
                            @note.Description
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <div class="col-md-4">
        <div class="panel-group" id="privateAccordion">
            @{i = 0;}
            @foreach (var note in ViewBag.privateList)
            {
                i++;
                <div class="panel panel-danger">
                    <div class="panel-heading">
                        <!-- panel-heading -->
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#privateAccordion" href=@("#privateAccordion" + i)>
                                @note.Title
                            </a>
                            <a href="/note/edit/@note.NoteID" type="button" class="btn btn-sm">修改</a>
                            <a href="/note/delete/@note.NoteID" type="button" class="btn btn-sm">删除</a>
                        </h4>
                    </div>
                    <!-- panel body -->
                    <div id=@("privateAccordion" + i) class="panel-collapse collapse">
                        <div class="panel-body">
                            @note.Description
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>

<button id="btn_model" style="display: none" data-toggle="modal" data-target="#deleteMsgModel"></button>
<div class="modal fade" id="deleteMsgModel">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" id="closs" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">笔记删除提醒</h4>
            </div>
            <div class="modal-body">
                <div class="alert alert-warning alert-dismissible fade in" role="alert">
                    <button class="close" aria-label="Close" type="button" data-dismiss="alert"><span aria-hidden="true">×</span></button>
                    <span>标题为 </span>
                    <span id="delete_title"><%= request.getAttribute("deleteNoteTitle")%></span>
                    <span>  的笔记已删除</span>
                </div><!--alert message-->
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="Done">确定</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script>
  $(document).on('ready', function() {
    $('#Done').click(function () { $('#closs').trigger('click'); });

    var deleteTitle = $('#delete_title').html();
    if (deleteTitle != 'null') {
      $('#btn_model').trigger('click');
    }
  });
</script>
